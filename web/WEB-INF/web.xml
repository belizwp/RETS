<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <description>ServletContextListener</description>
        <listener-class>listener.Init</listener-class>
    </listener>
    <servlet>
        <servlet-name>result</servlet-name>
        <jsp-file>/result.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>menu</servlet-name>
        <jsp-file>/member_menu.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>edit_profile</servlet-name>
        <jsp-file>/edit_profile.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>error</servlet-name>
        <jsp-file>/error.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>residential</servlet-name>
        <jsp-file>/residential.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>basic</servlet-name>
        <url-pattern>/basic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>detail</servlet-name>
        <url-pattern>/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>media</servlet-name>
        <url-pattern>/media</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>summary</servlet-name>
        <url-pattern>/summary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/result</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>menu</servlet-name>
        <url-pattern>/menu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>edit_profile</servlet-name>
        <url-pattern>/edit_profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>error</servlet-name>
        <url-pattern>/error/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>residential</servlet-name>
        <url-pattern>/residential</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <resource-ref>
        <description>jdbc:mysql://localhost:3306/rets?zeroDateTimeBehavior=convertToNull</description>
        <res-ref-name>retsdb</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
        <res-sharing-scope>Shareable</res-sharing-scope>
    </resource-ref>
    <servlet>
        <servlet-name>headerURL</servlet-name>
        <jsp-file>/templates/header.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>basic</servlet-name>
        <jsp-file>/new_announce1.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>detail</servlet-name>
        <jsp-file>/new_announce2.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>media</servlet-name>
        <jsp-file>/new_announce3.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>summary</servlet-name>
        <jsp-file>/new_announce4.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>headerURL</servlet-name>
        <url-pattern>/header</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>footerURL</servlet-name>
        <jsp-file>/templates/footer.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>footerURL</servlet-name>
        <url-pattern>/footer</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>registerURL</servlet-name>
        <jsp-file>/register.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerURL</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>loginURL</servlet-name>
        <jsp-file>/login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginURL</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>searchboxURL</servlet-name>
        <jsp-file>/searchbox/searchbox.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>searchboxURL</servlet-name>
        <url-pattern>/searchbox</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>searchURL</servlet-name>
        <jsp-file>/search.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>searchURL</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
</web-app>
